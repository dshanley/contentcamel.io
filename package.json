{
  "name": "contentcamel.io",
  "version": "1.3.0",
  "description": "The ContentCamel.io Website",
  "repository": "https://github.com/dshanley/contentcamel.io",
  "author": "dshanley",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "start": "gulp server",
    "preview": "cross-env NODE_ENV=production GENERATOR_ARGS=preview gulp server",
    "cc:preview": "cross-env NODE_ENV=production GENERATOR_ARGS=development gulp build",
    "cc:instant-preview": "cross-env NODE_ENV=production GENERATOR_ARGS=instantPreview gulp server",
    "build": "cross-env NODE_ENV=production gulp build",
    "preDeploy": "cross-env NODE_ENV=production gulp preDeploy",
    "redirect": "AWS_S3_BUCKET=www.contentcamel.io ./redirect.sh",
    "deploy": "cross-env NODE_ENV=production aws s3 sync ./dist s3://www.contentcamel.io --delete && npm run redirect",
    "clean": "gulp clean",
    "eslint": "./node_modules/eslint/bin/eslint.js src/js/ --ext .js",
    "eslint:fix": "./node_modules/eslint/bin/eslint.js src/js/ --ext .js --fix",
    "stylelint": "npm run stylelint:css && npm run stylelint:scss",
    "stylelint:fix": "npm run stylelint:css --fix && npm run stylelint:scss --fix",
    "stylelint:css": "./node_modules/stylelint/bin/stylelint.js src/css/**/*.css",
    "stylelint:scss": "./node_modules/stylelint/bin/stylelint.js src/scss/**/*.scss",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "algoliasearch": "^3.24.8",
    "atomic-algolia": "^0.2.1",
    "axios": "^0.18.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.26.5",
    "chalk": "^2.3.0",
    "clipboard": "^1.7.1",
    "compression": "^1.7.1",
    "content-loaded": "^1.1.0",
    "cross-env": "^5.1.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "dotenv": "^5.0.0",
    "eqcss": "^1.8.0",
    "exports-loader": "^0.6.3",
    "gulp": "^4.0.1",
    "gulp-babel": "^6.1.2",
    "gulp-debounce": "^1.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-newer": "^1.3.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gumshoejs": "^5.1.2",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.1",
    "hugo-bin": "^0.39.0",
    "laggard": "^2.0.1",
    "lazysizes": "^4.0.1",
    "ldclient-js": "^2.7.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cachebuster": "^0.1.5",
    "postcss-cssnext": "^2.7.0",
    "postcss-reporter": "^5.0.0",
    "postcss-sass": "^0.3.0",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "run-sequence": "^2.2.0",
    "shufflejs": "^5.2.2",
    "smoothscroll-polyfill": "^0.4.0",
    "stickyfilljs": "^2.0.3",
    "through2": "^2.0.3",
    "vinyl-named": "^1.1.0",
    "webpack": "3.6.0",
    "webpack-stream": "^4.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "imports-loader": "^0.8.0",
    "lint-staged": "^6.0.0",
    "pageres": "^6.2.3",
    "prettier": "1.19.1",
    "stylelint": "^10.0.1",
    "stylelint-config-sass-guidelines": "^4.0.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-no-unsupported-browser-features": "^1.0.1",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0"
  }
}
